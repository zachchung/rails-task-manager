<h1>New task</h1>

<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%#= f.label :completed %>
  <%#= f.check_box :completed %>
  <%= f.submit %>
<% end %>

<%= link_to "Back to list", tasks_path %>

